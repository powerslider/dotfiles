alias rm='rm -i'
alias mv='mv -i'

alias vim="stty stop '' -ixoff ; vim"
#'Freezing' tty, so after any command terminal settings will be restored
ttyctl -f

alias open=xdg-open

#IDEs
alias idea='$HOME/programs/idea/bin/idea.sh&'
alias ecl='$HOME/programs/eclipse/eclipse&'
alias clion='$HOME/programs/clion/bin/clion.sh&'
alias webstorm='$HOME/programs/webstorm/bin/webstorm.sh&'
alias pycharm='$HOME/programs/pycharm/bin/pycharm.sh&'

alias android='$HOME/programs/android-studio/bin/studio.sh&'
alias geny='$HOME/programs/genymotion/genymotion/genymotion&'

alias readlink=greadlink

